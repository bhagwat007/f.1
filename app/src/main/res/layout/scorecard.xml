<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_id"
    android:layout_width="match_parent"
    android:layout_height="65dp"
    android:background="#454A5E"
    android:backgroundTint="#343743">

    <TextView
        android:id="@+id/scoreout"
        android:layout_width="310dp"
        android:layout_height="20dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/sfnsdisplaylight1"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="346dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="6dp"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/scorebatsmenname"
            android:layout_width="178dp"
            android:layout_height="20dp"
            android:fontFamily="@font/nexa"
            android:gravity="center_vertical"
            android:text="@string/teama"
            android:textColor="#FFAB00"
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/scoreruns"
            android:layout_width="30dp"
            android:layout_height="28dp"
            android:gravity="center"
            android:text="TextView"
            android:textColor="@color/white"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/scoreballs"
            android:layout_width="30dp"
            android:layout_height="28dp"
            android:gravity="center"
            android:text="TextView"
            android:textColor="@color/white"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/score4s"
            android:layout_width="20dp"
            android:layout_height="28dp"
            android:gravity="center"
            android:text="TextView"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/scpre6s"
            android:layout_width="20dp"
            android:layout_height="28dp"
            android:gravity="center"
            android:text="TextView"
            android:textColor="@color/white"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/scoressr"
            android:layout_width="56dp"
            android:layout_height="28dp"
            android:gravity="center|center_vertical"
            android:text="@string/textview"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:textStyle="normal" />

    </TableRow>

    <View
        android:layout_width="335dp"
        android:layout_height="0.1dp"
        android:layout_marginTop="55dp"
        android:background="#F1EAED"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>